{
  "name": "@modern-js/codesmith",
  "description": "codesmith core implement",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/codesmith/issues",
  "repository": "modern-js-dev/codesmith",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.3.0",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --env=node"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/utils": "^1.7.8",
    "axios": "^0.21.1",
    "tar": "^6.1.1"
  },
  "devDependencies": {
    "@modern-js/module-tools": "^1.5.7",
    "@modern-js/plugin-testing": "^1.5.6",
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/tar": "^4.0.5",
    "typescript": "^4"
  },
  "modernConfig": {
    "output": {
      "packageMode": "node-js",
      "disableSourceMap": true
    }
  },
  "sideEffects": false
}
