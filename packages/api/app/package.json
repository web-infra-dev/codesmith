{
  "name": "@modern-js/codesmith-api-app",
  "description": "codesmith app api",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/codesmith/issues",
  "repository": "modern-js-dev/codesmith",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.2.5",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/codesmith": "workspace:^1.3.0",
    "@modern-js/codesmith-api-git": "workspace:^1.2.1",
    "@modern-js/codesmith-api-handlebars": "workspace:^1.2.1",
    "@modern-js/codesmith-api-npm": "workspace:^1.2.1",
    "@modern-js/easy-form-cli": "workspace:^1.2.2",
    "@modern-js/inquirer-types": "workspace:^1.2.2",
    "@modern-js/plugin-i18n": "^1.2.7",
    "@modern-js/utils": "^1.7.8",
    "comment-json": "^4.1.1",
    "extra": "^0.2.1",
    "inquirer": "8.1.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@modern-js/module-tools": "^1.5.7",
    "@modern-js/plugin-testing": "^1.5.6",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^26",
    "@types/lodash": "^4.14.172",
    "@types/node": "^14",
    "typescript": "^4"
  },
  "modernConfig": {
    "output": {
      "packageMode": "node-js",
      "disableSourceMap": true
    }
  },
  "sideEffects": false
}
