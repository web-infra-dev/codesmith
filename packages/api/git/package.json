{
  "name": "@modern-js/codesmith-api-git",
  "description": "codesmith git api",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/codesmith/issues",
  "repository": "modern-js-dev/codesmith",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.0.2",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/codesmith": "workspace:^1.0.2",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@modern-js/module-tools": "^1.0.0-rc.2",
    "@modern-js/plugin-testing": "^1.0.0-rc.2",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26",
    "@types/node": "^14",
    "typescript": "^4"
  },
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "sideEffects": false
}
