{
  "name": "@modern-js/codesmith-api-npm",
  "description": "codesmith npm api",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/codesmith/issues",
  "repository": "modern-js-dev/codesmith",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.6.3",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/codesmith": "workspace:*",
    "@modern-js/utils": "^1.19.0"
  },
  "devDependencies": {
    "@modern-js/module-tools": "^1.19.0",
    "@modern-js/plugin-testing": "^1.19.0",
    "@types/jest": "^26",
    "@types/node": "^14",
    "typescript": "^4"
  },
  "modernConfig": {
    "output": {
      "packageMode": "node-js",
      "disableSourceMap": true
    }
  },
  "sideEffects": false
}
