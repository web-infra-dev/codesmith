{
  "name": "@modern-js/codesmith-formily",
  "description": "codesmith support formly schema to inquirer question",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/codesmith/issues",
  "repository": "modern-js-dev/codesmith",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.0.3",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@formily/json-schema": "^2.2.3",
    "@formily/validator": "^2.2.6",
    "@modern-js/utils": "^2.4.0",
    "inquirer": "^8"
  },
  "peerDependencies": {
    "@modern-js/codesmith": "workspace:^2.0.3"
  },
  "devDependencies": {
    "@modern-js/codesmith": "workspace:*",
    "@modern-js/module-tools": "^2.4.0",
    "@modern-js/plugin-testing": "^2.4.0",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^26",
    "@types/node": "^14",
    "typescript": "^4"
  },
  "sideEffects": false
}
